{
  "name": "kogera-reader",
  "version": "1.2.0",
  "description": "qrcode reader with kxing",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf es dist",
    "start": "webpack-dev-server",
    "build": "webpack",
    "build:min": "webpack --optimize-minimize --define process.env.NODE_ENV=\"'production'\" --output-filename bundle.min.js",
